    "Name": "G2",
    "Texts": [
        {
            "Key": "DIA_Addon_Xardas_Hello_14_00",
            "Texts": [
                {
                    "Language": "English",
                    "Text": "Da bist du ja wieder! (schmunzelt) Ich hätte nicht gedacht, daß wir uns nochmal wiedersehen."
                },
                {
                    "Language": "German",
                    "Text": "Da bist du ja wieder! (schmunzelt) Ich hätte nicht gedacht, daß wir uns nochmal wiedersehen."
                }
            ]
        },
        {
            "Key": "DIA_Addon_Xardas_Hello_15_01",
            "Texts": [
                {
                    "Language": "English",
                    "Text": "Ich fühle mich, als hätte ich drei Wochen unter Steinen gelegen."
                },
                {
                    "Language": "German",
                    "Text": "Ich fühle mich, als hätte ich drei Wochen unter Steinen gelegen."
                }
            ]
        },
        {
            "Key": "DIA_Addon_Xardas_Hello_14_02",
            "Texts": [
                {
                    "Language": "English",
                    "Text": "So war es auch. Nur die Magie deiner Rüstung hat dich am Leben gehalten "
                },
                {
                    "Language": "German",
                    "Text": "So war es auch. Nur die Magie deiner Rüstung hat dich am Leben gehalten "
                }
            ]
        },
        {
            "Key": "DIA_Addon_Xardas_Hello_14_03",
            "Texts": [
                {
                    "Language": "English",
                    "Text": "Ich hatte schon befürchtet, dich nicht mehr aus den Trümmern des Tempels befreien zu können."
                },
                {
                    "Language": "German",
                    "Text": "Ich hatte schon befürchtet, dich nicht mehr aus den Trümmern des Tempels befreien zu können."
                }
            ]
        },
        {
            "Key": "DIA_Addon_Xardas_Hello_14_04",
            "Texts": [
                {
                    "Language": "English",
                    "Text": "Aber genug davon. Jetzt bist du ja hier."
                },
                {
                    "Language": "German",
                    "Text": "Aber genug davon. Jetzt bist du ja hier."
                }
            ]
        },
        {
            "Key": "DIA_Addon_Xardas_Hello_14_05",
            "Texts": [
                {
                    "Language": "English",
                    "Text": "Es gibt eine NEUE Bedrohung, um die wir uns kümmern müssen."
                },
                {
                    "Language": "German",
                    "Text": "Es gibt eine NEUE Bedrohung, um die wir uns kümmern müssen."
                }
            ]
        }
    ],
